<h1>New Item Form</h1>

<%= form_for @item do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :price %>
  <%= f.text_field :price %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :item_picture_url %>
  <%= f.file_field :item_picture_url, class: "form-control"%>

  <%= f.submit "Add Item"%>
<% end %>
